  {{#each hits}}
  <li>
  <div class="photo-card" style="border:1px solid #000;">
    <img width= "280" height="200" class="noLoad" id="{{this.id}}" src="{{this.webformatURL}}" alt="{{this.type}}"
      data-origin="{{this.largeImageURL}}">
    <div class="stats">
      <p class="stats-item">
        <i class="material-icons">thumb_up</i>
        {{this.likes}}
      </p>
      <p class="stats-item">
        <i class="material-icons">visibility</i>
        {{this.views}}
      </p>
      <p class="stats-item">
        <i class="material-icons">comment</i>
        {{this.comments}}
      </p>
      <p class="stats-item">
        <i class="material-icons">cloud_download</i>
        {{this.downloads}}
      </p>
    </div>
  </div>
  </li>
  {{/each}}
